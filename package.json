{
  "name": "example-with-drizzle",
  "type": "module",
  "private": true,
  "scripts": {
    "check": "biome check",
    "check:fix": "biome check --write",
    "format": "biome format --write",
    "format:check": "biome format",
    "lint": "bun check",
    "lint:fix": "bun fix",
    "dev": "vinxi dev",
    "build": "vinxi build",
    "start": "vinxi start",
    "preview": "bunx dotenv run bun start",
    "auth:generate": "bunx @better-auth/cli generate --config ./src/features/authentication/lib/auth.ts --output ./src/features/authentication/drizzle/schema.ts",
    "drizzle:generate": "bun auth:generate && drizzle-kit generate",
    "drizzle:migrate": "drizzle-kit migrate",
    "drizzle:create": "bun drizzle:generate; bun drizzle:migrate",
    "drizzle:push": "drizzle-kit push",
    "drizzle:studio": "drizzle-kit studio",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "bun playwright test --ui",
    "test:ci": "bun test; bun playwright test"
  },
  "devDependencies": {
    "@better-auth/cli": "^1.2.7",
    "@biomejs/biome": "1.9.4",
    "@playwright/test": "^1.52.0",
    "@types/better-sqlite3": "^7.6.13",
    "dotenv-cli": "^8.0.0",
    "drizzle-kit": "^0.31.1",
    "tailwindcss": "^4.1.5",
    "tailwindcss-motion": "1.1.0",
    "tsx": "^4.19.4",
    "vitest": "^3.1.2"
  },
  "dependencies": {
    "@kobalte/core": "^0.13.9",
    "@solid-primitives/i18n": "^2.2.1",
    "@solid-primitives/refs": "^1.1.1",
    "@solid-primitives/storage": "^4.3.2",
    "@solidjs/router": "^0.15.3",
    "@solidjs/start": "^1.1.3",
    "better-auth": "^1.2.7",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.36.4",
    "solid-js": "^1.9.6",
    "solid-server": "^5.8.5",
    "tailwind-merge": "^3.2.0",
    "uuid": "^11.1.0",
    "vinxi": "^0.5.6",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "engines": {
    "node": ">=20"
  },
  "trustedDependencies": [
    "@biomejs/biome",
    "@parcel/watcher",
    "core-js"
  ]
}